<!DOCTYPE html>
<html lang="fr">
  <%- include('_head', {title: appName }); %>
  <body>
    <main class="container">
      <%- include('_nav'); %>
      <h1 style="display: none"><%= appName %></h1>

      <h2>Code postaux</h2>

      <ul>
        <% zipCodes.forEach(function ({zipCode, name}) { %>
        <li><a href="<%= zipCode %>"><%= name %> (<%= zipCode %>)</a></li>
        <% }); %>
      </ul>

      <h2>Foire aux questions</h2>

      <%- include('_about'); %>
    </main>
  </body>
</html>
